{
	// Use IntelliSense to learn about possible attributes.
	// Hover to view descriptions of existing attributes.
	// For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch Chrome",
			"request": "launch",
			"type": "pwa-chrome",
			"url": "http://localhost:8080",
			"webRoot": "${workspaceFolder}"
		},
		{

			"name": "Main Download and Run",
			"type": "ev3devBrowser",
			"request": "launch",
			"program": "/home/robot/${workspaceRootFolderName}/main.py",
			"url": "http://localhost:8080",
			"interactiveTerminal": false
		}, 
		{
			"name": "ilan-brother",
			"type": "ev3devBrowser",
			"request": "launch",
			"program": "/home/robot/${workspaceRootFolderName}/ilan-brother.py",
			"interactiveTerminal": false
		}, 
		{
			"name": "Wall Download and Run",
			"type": "ev3devBrowser",
			"request": "launch",
			"program": "/home/robot/${workspaceRootFolderName}/wall.py",
			"interactiveTerminal": false
		}
		, 
		{
			"name": "Runs Download and Run",
			"type": "ev3devBrowser",
			"request": "launch",
			"program": "/home/robot/${workspaceRootFolderName}/runs.py",
			"interactiveTerminal": true
		}
		, 
		{
			"name": "TESTS Download and Run",
			"type": "ev3devBrowser",
			"request": "launch",
			"program": "/home/robot/${workspaceRootFolderName}/TESTS.py",
			"interactiveTerminal": false
		}
	]
}
